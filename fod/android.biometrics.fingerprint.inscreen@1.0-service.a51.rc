service fingerprint-inscreen-1-0 /system/bin/hw/android.biometrics.fingerprint.inscreen@1.0-service.a51
    interface vendor.android.biometrics.fingerprint.inscreen@1.0::IFingerprintInscreen default
    class hal
    user system
    group system
    shutdown critical

on boot
    # FOD Brightness
    chown system system /sys/class/lcd/panel/mask_brightness
    chmod 0644 /sys/class/lcd/panel/mask_brightness

    # TSP Command
    chown system system /sys/class/sec/tsp/cmd
